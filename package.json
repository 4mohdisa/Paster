    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.6.0",
    "fast-deep-equal": "^3.1.3",
    "geist": "^1.4.2",
    "get-port-please": "^3.1.2",
    "input-otp": "^1.4.2",
    "ioredis": "^5.6.1",
    "lucide-react": "^0.511.0",
    "motion": "^12.12.1",
    "nanoid": "^5.1.5",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
