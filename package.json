    "zod": "^3.24.4",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/d3-scale": "^4.0.9",
    "@types/node": "^22.15.18",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@types/tar": "^6.1.13",
    "drizzle-kit": "^0.31.1",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.27.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.7",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5.8.3"
  },
  "build": {
    "asar": true,
    "executableName": "NextJSElectron",
    "appId": "com.aman.nextjs-electron",
    "directories": {
      "buildResources": "electron-build-assets"
    },
    "asarUnpack": [
      "node_modules/next",
      "node_modules/@img",
      "node_modules/sharp",
      "resources/**",
      "**\\*.{node,dll}"
    ],
    "files": [
      "build",
      "resources/**",
      {
        "from": ".next/standalone",
        "to": "app",
        "filter": [
          "!**/.env",
          "!**/package.json"
        ]
      },
      {
        "from": ".next/static",
        "to": "app/.next/static"
      },
      {
        "from": "public",
        "to": "app/public"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Development"
    }
  }
}
